.map_box {
    position: absolute;
    top: 0px;
    bottom: 50px;
    left: 0px;
    right: 0px;
}

#navi_map {
    width: 100%;
    height: 100%;
}

.text_box {
    position: absolute;
    height: 50px;
    bottom: 0px;
    left: 0px;
    right: 0px;
    background-color: #fff;
    box-shadow: 0 -4rpx 10rpx #eee;
    line-height: 50px;
    padding-left: 10px;
}

.text_box .text {
    margin: 15px;
}

.detail_button {
    position: absolute;
    bottom: 30px;
    right: 10px;
    padding: 3px 5px;
    color: #fff;
    background: #0091ff;
    width: 50px;
    text-align: center;
    border-radius: 5px;
}

.carCheck {
    position: absolute;
    top: 20rpx;
    width: calc(90% - 40rpx);
    margin-left: 5%;
    background-color: #fff;
    border-radius: 6rpx;
    line-height: 100rpx;
    padding-left: 40rpx;
    height: 100rpx;
    z-index: 10000;
    box-shadow: 0 0 20rpx #ddd;
}

image {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    display: inline-block;
    right: 40rpx;
    margin-top: 25rpx;
}