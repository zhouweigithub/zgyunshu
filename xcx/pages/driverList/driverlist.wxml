<view style="width:100%;height:100%;position:fixed">
    <view style="width: 100%;height: 260rpx;background: #fff;border-bottom:1rpx solid #f2f2f2;position: relative;">
        <view>
            <view style="position: absolute;top:50%;left:60rpx;margin-top: -10rpx;color:#535353">{{username}}</view>
        </view>
        <image src="{{header}}" style="width: 140rpx;height:140rpx;border-radius: 50%;border:1rpx solid #f6f6f6;position: absolute;right: 100rpx;top:50%;margin-top: -70rpx"></image>
    </view>
    <view bindtap="navToCont" class="item">
        <view class="imageItem">
            <image src="../../images/orderManage.png"></image>
        </view>
        <view class="title">
            <text>我的订单</text>
        </view>
        <view class="num">
            <text>34</text>
        </view>
    </view>
    <view bindtap="navToMsg" class="item">
        <view class="imageItem">
            <image src="../../images/orderManage.png"></image>
        </view>
        <view class="title">
            <text>车主</text>
        </view>
        <view class="num"></view>
    </view>
</view>
