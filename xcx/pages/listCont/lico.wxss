.li_info {
    height: 300rpx;
    width: 94%;
    background-color: rgb(52,194,124);
    color: #fff;
    margin-left: 3%;
    border-radius: 8rpx;
    margin-top: 10rpx;
    position: relative;
}

.item {
    width: 30rpx;
    height: 30rpx;
    border-radius: 15rpx;
    background: rgba(255,255,255,0.3);
    position: absolute;
    top: 20rpx;
    right: 50rpx;
}

.addre {
    position: absolute;
    margin-left: 300rpx;
    margin-top: -15rpx;
    font-size: 32rpx;
    top: 120rpx;
    width: 170rpx;
}

.price {
    position: absolute;
    font-size: 45rpx;
    display: inline;
    margin-top: 150rpx;
    right: 50rpx;
}

.shaft {
    position: absolute;
    bottom: 20rpx;
    left: 50rpx;
}

.num {
    position: absolute;
    top: 80rpx;
    right: 50rpx;
    font-size: 40rpx;
}

.num view {
    display: inline-block;
    font-size: 28rpx;
}

.time {
    position: absolute;
    bottom: 20rpx;
    right: 50rpx;
    font-size: 30rpx;
}

.distance {
    position: absolute;
    left: 80rpx;
    top: 80rpx;
}

image {
    width: 240rpx;
    position: absolute;
    height: 60rpx;
    left: 50rpx;
    top: 100rpx;
}

.limit {
    position: absolute;
    top: 0;
    border-top-left-radius: 8rpx;
    border-bottom-right-radius: 60rpx;
    background: #535353;
    box-shadow: 2rpx 2rpx 4rpx rgba(0,0,0,0.3);
    font-size: 28rpx;
    display: inline-block;
    padding: 10rpx;
    padding-right: 40rpx;
}

.hint {
    margin-top: 20rpx;
    background: #535353;
    height: 40rpx;
    width: 17rpx;
    display: inline-block;
}

.itemCon {
    width: 90%;
    margin-left: 5%;
    border: 1rpx solid #f0f0f0;
    border-radius: 8rpx;
    position: relative;
    height: 200rpx;
    margin-top: 30rpx;
}

.name {
    position: absolute;
    top: 30rpx;
    font-size: 30rpx;
    color: #535353;
    left: 30rpx;
}

.material {
    position: absolute;
    color: #535353;
    bottom: 30rpx;
    left: 30rpx;
    font-size: 30rpx;
}

.rules {
    font-size: 30rpx;
    position: absolute;
    bottom: 30rpx;
    left: 240rpx;
}

.id {
    border: 1rpx solid #f0f0f0;
    height: 40rpx;
    padding: 10rpx;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    font-size: 30rpx;
}

.mNum {
    display: inline-block;
    font-size: 30rpx;
    position: absolute;
    right: 30rpx;
    bottom: 20rpx;
}

.limits {
    width: 100%;
    height: 100rpx;
    margin-top: 10rpx;
    text-align: center;
    display: flex;
    flex-direction: row;
}

.limit_item {
    flex: 1;
}

.limit_item view {
    display: inline-block;
    font-size: 32rpx;
}

.Lhint {
    width: 20rpx;
    height: 20rpx;
    border-radius: 10rpx;
    background: #09bb07;
    margin-top: 40rpx;
    margin-right: 10rpx;
}

