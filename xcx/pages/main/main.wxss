image {
    width: 100%;
    height: 100%;
}

.item_nav {
    width: 340rpx;
    height: 300rpx;
    background: #fff;
    border-radius: 4rpx;
    border: 1rpx solid #f2f2f2;
    margin-left: 20rpx;
    margin-top: 10rpx;
    display: inline-block;
}

.item_nav image {
    width: 100rpx;
    height: 100rpx;
    margin-top: 70rpx;
    margin-left: 120rpx;
}

.item_nav view {
    font-size: 30rpx;
    color: #535353;
    text-align: center;
    margin-top: 20rpx;
}